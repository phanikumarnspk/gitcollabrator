package com.slokam1;

import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;

public class two {
	
	public static void compiletime() throws IOException{
		
		System.out.println("Hai this is compiletime= IOException");
		FileReader fr = new FileReader("C:\\slokam\\FILE\\product.txt");
		BufferedReader br = new BufferedReader(fr);
		for(String s = br.readLine(); s != null; s = br.readLine()) {
			
			System.out.println(s);
		}
	}
	
	public static void runtime1()throws NullPointerException{
		
		String i = null;
		
		System.out.println("Hai this is get2 NullpointException");
		
		System.out.println(i.length());
	}
	
	public static void runtime()throws ArithmeticException{
		
		System.out.println("Hai this is get3 ArithmaticException");
		
		System.out.println(10/0);
	}

	public static void main(String[] args) {
		

		try {
			compiletime();
		} catch (IOException e) {
			
			e.printStackTrace();
		}
		
		runtime();
		runtime1();
	}

}
